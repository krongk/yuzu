  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/waypoints.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/owl.carousel.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/jquery.touchSwipe.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/jquery.shuffle.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/jquery-numerator.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/lightGallery.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/plugins/grid.js'></script>

  <script type='text/javascript' src='/themes/domino//wp-content/plugins/domino-core/assets/js/domino-core.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/themes/domino/js/plugins/waypoints-sticky.min.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/themes/domino/js/plugins/smoothscroll.js'></script>
  <script type='text/javascript' src='/themes/domino//wp-content/themes/domino/js/custom.js'></script>

   
  </body>

